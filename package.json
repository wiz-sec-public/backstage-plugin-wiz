{
  "name": "backstage-plugin-wiz",
  "version": "1.0.4",
  "private": true,
  "engines": {
    "node": "24"
  },
  "scripts": {
    "dev": "dotenv -- pnpm -r start",
    "start": "pnpm -r --filter @wiz-sec/backstage-plugin-wiz start",
    "start:backend": "pnpm -r --filter @wiz-sec/backstage-plugin-wiz-backend start",
    "build": "backstage-cli repo build",
    "clean": "backstage-cli repo clean",
    "test": "jest",
    "lint": "eslint --cache",
    "tsc": "tsc --build tsconfig.json",
    "tsc:clean": "tsc --build --clean"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "plugins/*"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.5",
    "@eslint/js": "^9.39.1",
    "@spotify/prettier-config": "^15.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.3",
    "concurrently": "^9.2.1",
    "cross-fetch": "^4.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "taze": "^19.9.2",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "prettier": "@spotify/prettier-config",
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "overrides": {
      "tar-fs": "3.1.1",
      "jws": "4.0.1",
      "@octokit/request-error": "5.1.1",
      "@octokit/request": "8.4.1",
      "@octokit/plugin-paginate-rest": "14.0.0",
      "prismjs": "1.30.0",
      "koa": "2.16.3",
      "qs": "6.14.1",
      "@remix-run/router": "1.23.2",
      "tar": "7.5.7",
      "fast-xml-parser": "5.3.4",
      "lodash": "4.17.23",
      "@backstage/cli-common": "0.1.17",
      "diff": "5.2.2",
      "undici": "7.18.2"
    }
  }
}
